{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "align": false,
        "curly": false,
        "member-ordering": false,
        "no-console": false,
        "object-literal-shorthand": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "quotemark": false,
        "semicolon": false,
        "trailing-comma": false,
        "variable-name": false
    },
    "rulesDirectory": []
}